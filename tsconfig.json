{
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmitOnError": false,
    "target": "es5",
    "lib": ["dom", "es2015"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
  },
  "include": ["src"]
}
